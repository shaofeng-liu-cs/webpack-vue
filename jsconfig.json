{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "es5",
        "module": "commonjs",
        "jsx": "preserve",
        "allowSyntheticDefaultImports": true,
        "lib": ["es6", "dom"],
        "paths": {
            "@/*": ["src/*"]
        }
    },
    "exclude": ["node_modules", "bower_components", "jspm_packages", "tmp", "temp"],
    "typeAcquisition": {
        "enable": true
    }
    
}
